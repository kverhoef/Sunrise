<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "nl.kevinverhoef.sunrise"
        version   = "1.0">

    <name>Sunrise</name>

    <description>
        Sunrise
    </description>

    <author href="http://kevinverhoef.nl" email="verhoef.k@gmail.com">
        Kevin Verhoef
    </author>
    
    <gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <gap:plugin name="com.sharinglabs.cordova.plugin.datepicker" version="1.1.3" />
    
	<icon src="icon.png" />
    <preference name="phonegap-version" value="3.6.3" />

    <preference name="fullscreen" value="true" />
    <preference name="detect-data-types" value="false" />
    <preference name="exit-on-suspend" value="true" />

    <gap:platform name="android" />
    
    <access origin="https://*.yahooapis.com" />
    <access origin="https://query.yahooapis.com" />
</widget>
